	describe("#constructor", function() {

		it('should remember its access token', function() {
			let driver = new FacebookDriver(new Identity({
				oauthSecret: 'cookies&c_user=123',
				oauthToken: 'access-token',
				userId: 'user-id'
			}));
			driver.identity.oauthSecret.should.equal('cookies&c_user=123');
			driver.identity.oauthToken.should.equal('access-token');
			driver.identity.userId.should.equal('user-id');
		});

		it('should complain vociferously if an identity is not supplied', function() {
			chai.expect(FacebookDriver).to.throw(Error);
		});

		it('should complain vociferously if an access token is not supplied', function() {
			chai.assert.throws(() => new FacebookDriver(new Identity({
				oauthToken: null
			})), Error);
		});

		it('should complain vociferously if cookies are not supplied', function() {
			chai.assert.throws(() => new FacebookDriver(new Identity({
