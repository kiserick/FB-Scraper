// Shortcut to check a 'graph.facebook.com' HTTP request.
let __checkGraphRequest = (expectedPath, response, expectedExtra) => subject.http.checkHttpRequest('graph.facebook.com', expectedPath, response, expectedExtra);

// Helpers to generate parts of a URI expected as a part of the scraping subsystem.
let __generateBatchUrl = (values) => {
	let mValues = ['batch=[', 'access_token=access-token'];
	values.forEach((value) => {
		let req = {
			method: 'GET',
			relative_url: ('v2.5/' + value + '?fields=story,id,type,updated_time,message,link,full_picture,name,status_type,source,actions,application,likes.limit(100),attachments,from{name,id,picture,likes},to,comments.limit(12){id,from{id,name,picture},message,created_time}')
		}
		mValues.push(JSON.stringify(req));
	});

	return mValues;
}
let batch = (values) => ['/', __generateBatchUrl(values)];

